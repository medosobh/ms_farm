<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record model="res.users" id="base.default_user">
        <field name="groups_id" eval="[(4,ref('group_account_manager'))]"/>
    </record>
    <record model="res.users" id="base.user_admin">
        <field name="groups_id" eval="[(4, ref('account.group_account_user'))]"/>
    </record>
    <record model="res.config.settings" id="res_config_settings">
        <field name="group_uom" eval="1"/>
        <field name="group_stock_multi_locations" eval="1"/>
        <field name="group_analytic_accounting" eval="1"/>
    </record>
    <function model="res.config.settings"
              name="execute"
              eval="[ref('res_config_settings')]">
    </function>
</odoo>